#include <iostream>
using namespace std;
long x;
long y;

long long SuperPower2(long a,long b){
if (b==0){
  return 1;
}
else {
  return a*SuperPower2(a,b-1);
}
}

int main(){
std::cout << "Please enter an integer: ";
std::cin >> x;
std::cout << "Please enter another integer: ";
std::cin >> y;
std::cout << SuperPower(x,y) << std::endl;
std::cout << SuperPower2(x,y);

return 0;
}
